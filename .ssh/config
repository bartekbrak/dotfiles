# man ssh:
#    For each parameter, the first obtained value will be used.
# IdentityFile:
# - full path to the private key file
# - using ~ is fine
#
# Host matches whatever is used in the commenadline
# HostName is the actual target

Include config.d/*
IdentitiesOnly yes
ServerAliveInterval 5

Host bitbucket.org.priv priv.bitbucket.org
    HostName bitbucket.org
    IdentityFile ~/.ssh/bartek.rychlicki@gmail.com

Host github.com
    HostName github.com
    IdentityFile ~/.ssh/bartek.rychlicki@gmail.com

Host mints
    HostName 185.180.206.68
    User root
    IdentityFile ~/.ssh/bartek.rychlicki@gmail.com

Host kinro
    HostName 185.180.206.68
    User kinro
    IdentityFile ~/.ssh/bartek.rychlicki@gmail.com

Host kiu_server
    HostName 185.180.206.68
    User kiu_server
    IdentityFile ~/.ssh/bartek.rychlicki@gmail.com

Host yong
    HostName 185.180.206.68
    User yong
    IdentityFile ~/.ssh/bartek.rychlicki@gmail.com

Host contabo
    HostName 5.189.150.20
    User root
    IdentityFile ~/.ssh/bartek.rychlicki@gmail.com
# I will name all services that have my key so that I remember
